-
	var jsLibs= {	
		redux: 'https://cdnjs.cloudflare.com/ajax/libs/redux/3.7.2/redux.min.js',
		react: 'https://cdnjs.cloudflare.com/ajax/libs/react/15.6.1/react.min.js',
		"react-dom": 'https://cdnjs.cloudflare.com/ajax/libs/react/15.6.1/react-dom.min.js',
		'.min.js': 'https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.4.2/js/swiper.min.js',
		'react-redux': 'https://cdnjs.cloudflare.com/ajax/libs/react-redux/5.0.6/react-redux.min.js',
		'react-router-DOM': 'https://cdnjs.cloudflare.com/ajax/libs/react-router-dom/4.2.2/react-router-dom.min.js'
	}
	var cssLibs= {
		normalize : 'https://cdnjs.cloudflare.com/ajax/libs/normalize/7.0.0/normalize.min.css'
		}

html
	head
		if( jsLibs )
			each lib,libName in jsLibs
				script(src=lib)
				//.
					#{libName}
		if( cssLibs )
			each lib,libName in cssLibs
				link(rel='stylesheet', href=lib)
				//.
					#{libName}
		
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
	body
		div#container      	     